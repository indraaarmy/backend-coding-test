    "winston": "^3.5.1"
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "mocha": "^6.1.4",
    "nyc": "^15.1.0",
    "pre-push": "^0.1.1",
    "supertest": "^4.0.2"
  }
}
